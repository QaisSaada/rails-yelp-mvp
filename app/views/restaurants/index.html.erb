<p style="color: green"><%= notice %></p>

<img src="data:image/png;base64," class="img-fluid" alt="...">

<div id="restaurants">
  <% @restaurants.each do |restaurant| %>
    <%= render restaurant %>
    <p>
      <%= link_to "Show this restaurant", restaurant, class: "btn btn-warning" %>
    </p>
  <% end %>
</div>

<h2>Help us register new restaurants</h2>
<%= link_to "New restaurant", new_restaurant_path, class: "btn btn-warning"%>
